{
  "manifest_version": 3,
  "name": "Simple Volume Booster",
  "version": "2.1",
  "description": "A Chrome extension for Dejan",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}